        "else:\n",
        "    print(\"No text was extracted.\")\n",
        "\n",
        "## Consolidate extracted text\n",
        "all_combined_text = \" \".join(extracted_texts)\n",
        "print(f\"\\nTotal number of characters in all extracted text: {len(all_combined_text)}\")"
